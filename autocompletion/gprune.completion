_gprune() 
{
    local cur prev opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    prev="${COMP_WORDS[COMP_CWORD-1]}"
    opts="--help --repository"

    if [[ ${cur} == -* ]] ; then
        COMPREPLY=( $(compgen -W "${opts}" -- ${cur}) )
        return 0
    fi

    if [[ ${prev} == "--repository"  ]]; then
      COMPREPLY=( $(compgen -W "$(ls "$(pwd)")" -- ${cur}) )
      return 0
    fi    
}

complete -F _gprune gprune